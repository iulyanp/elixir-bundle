{
    "name": "iulyanp/elixir-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that integrates Laravel Elixir",
    "keywords": ["symfony3", "elixir", "symfony-elixir"],
    "license": "MIT",
    "authors": [
        {
            "name": "Iulian Popa",
            "email": "iulyanpopa@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Iulyanp\\ElixirBundle\\": ""
        }
